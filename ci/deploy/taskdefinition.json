[
  {
    "name": "${service_name}",
    "image": "judo/additions:${image_tag}",
    "memory": 256,
    "portMappings": [
      {
        "containerPort": 80
      }
    ],
    "environment" : [
      { "name": "ASPNETCORE_URLS", "value": "http://*:80" },
      { "name": "ASPNETCORE_ENVIRONMENT", "value": "${environment}" },
      { "name": "CASSANDRA__DATACENTER", "value": "eu-west" },
	    { "name": "BUILD_ID", "value": "${build_id}"}
    ]
  }
]
